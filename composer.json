{
    "name": "nwidart/forecast-php",
    "description": "PHP client package for the Forecast.io service",
    "keywords": [
        "php",
        "Forecast.io",
        "Forecast",
        "Weather"
    ],
    "homepage": "https://github.com/nWidart/forecast-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Widart",
            "email": "n.widart@gmail.com",
            "homepage": "http://www.nicolaswidart.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "guzzlehttp/guzzle": "~5.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.7",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Nwidart\\ForecastPhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nwidart\\ForecastPhp\\tests\\": "tests"
        }
    }
}
